create Database asg

CREATE TABLE [dbo].[EAESUser](
	[userId] [int] IDENTITY(1,1) NOT NULL ,
	[username] [varchar](255) NOT NULL unique,
	[encryptedPassword] [varchar](255) NULL,
	[ECCpublickey] [varchar](255) NOT NULL
)

ALTER TABLE EAESUser
ADD Date_Created datetime DEFAULT GETDATE();

CREATE TABLE KeyVault (
    [keyId] INT IDENTITY(1,1) NOT NULL PRIMARY KEY,
    [keyValue] varchar(255) NOT NULL,
    [username] VARCHAR(255) NOT NULL,
    FOREIGN KEY (username) REFERENCES EAESUser(username)
);
